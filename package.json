{
  "name": "react-resume",
  "version": "0.2.0",
  "description": "Create a PDF resume using React",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build",
    "dev": "vite",
    "prsbuild": "npm run clean && mkdir build",
    "build": "vite build && npm run vrun",
    "vrun": "node dist/index.mjs",
    "postbuild": "mv cv.pdf build/cv.pdf",
    "clean": "rm -rf ./build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonybudianto/react-resume.git"
  },
  "author": "Antony Budianto <antonybudianto@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonybudianto/react-resume/issues"
  },
  "homepage": "https://github.com/antonybudianto/react-resume#readme",
  "dependencies": {
    "@react-pdf/renderer": "4.0.0",
    "prop-types": "15.8.1",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react-pdf": "^6.2.0",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.0.1",
    "vite": "^5.4.10"
  }
}
